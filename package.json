{
  "name": "bsky-event-handlers",
  "version": "0.4.63-prerelease.1",
  "description": "Utilities to help handle bluesky events with validation",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "format": "npx prettier . --write",
    "lint": "eslint --ext=.ts src/ tests/ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juni-b-queer/bsky-event-handlers.git"
  },
  "keywords": [
    "bluesky"
  ],
  "author": "Juniper Bender",
  "license": "BSD-4-Clause",
  "bugs": {
    "url": "https://github.com/juni-b-queer/bsky-event-handlers/issues"
  },
  "homepage": "https://github.com/juni-b-queer/bsky-event-handlers#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-date-mock": "^1.0.8",
    "jest-mock-extended": "^3.0.5",
    "prettier": "3.2.4",
    "semantic-release": "^22.0.12",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@atproto/api": "^0.7.4",
    "atproto-firehose": "^0.2.2"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist/*.{js,ts,mjs,mts}",
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "branches": [
      {
        "name": "main",
        "prerelease": false
      },
      {
        "name": "beta",
        "prerelease": "prerelease"
      }
    ]
  }
}
